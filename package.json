{
  "name": "drama-queen",
  "private": true,
  "version": "3.2.4-rc.roundabout-continue",
  "type": "module",
  "scripts": {
    "generate-routes": "tsr generate",
    "dev": "pnpm run generate-routes && vite --port 5001 --strictPort",
    "test": "vitest run",
    "check": "tsc",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "tsc && pnpm run generate-routes && vite build",
    "serve": "vite serve --port 5001 --strictPort",
    "postbuild": "node build/remote-env.cjs remoteEntry.js",
    "preview": "vite preview --port 5001 --strictPort",
    "_format": "prettier --ignore-unknown .",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --list-different",
    "lint": "eslint ./src",
    "stop": "kill-port --port 5000,5001,5002",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@inseefr/lunatic": "^3.11.1",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-date-pickers": "^7.28.2",
    "@tanstack/history": "^1.154.14",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.16",
    "@types/memoizee": "^0.4.11",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "dexie": "^3.2.7",
    "dexie-react-hooks": "^1.1.7",
    "evt": "^2.5.7",
    "i18next": "^25.8.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jwt-decode": "^4.0.0",
    "oidc-spa": "6.8.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.51.1",
    "react-hot-keys": "^2.7.3",
    "react-i18next": "^16.5.4",
    "redux-clean-architecture": "^4.3.2",
    "tsafe": "^1.6.6",
    "tss-react": "^4.9.13",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@originjs/vite-plugin-federation": "^1.2.3",
    "@tanstack/router-cli": "^1.157.16",
    "@tanstack/router-plugin": "^1.158.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "fake-indexeddb": "^6.2.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^16.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.0",
    "vite-envs": "^4.6.0",
    "vite-plugin-pwa": "^0.21.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --list-different"
    ],
    "!(*.js|*.jsx|*.ts|*.tsx)": "prettier --write --ignore-unknown --list-different"
  }
}
